{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "AIWareFlow": {
      "projectType": "application",
      "schematics": {},
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular/build:application",
          "options": {
            "browser": "src/main.ts",
            "tsConfig": "tsconfig.app.json",
            "assets": [
              "src/assets",
              {
                "glob": "**/*",
                "input": "public"
              }
            ],
            "styles": [
              "src/assets/css/bootstrap.min.css",
              "src/assets/css/bootstrap-datetimepicker.min.css",
              "src/assets/css/animate.css",
              "src/assets/plugins/select2/css/select2.min.css",
              "src/assets/plugins/tabler-icons/tabler-icons.min.css",
              "src/assets/plugins/fontawesome/css/fontawesome.min.css",
              "src/assets/plugins/fontawesome/css/all.min.css",
              "src/assets/plugins/@simonwep/pickr/themes/nano.min.css",
              "src/assets/css/style.css",
              "node_modules/bootstrap-icons/font/bootstrap-icons.css",
              "src/assets/css/dataTables.bootstrap5.min.css"

            ],
            "scripts": [
              "src/assets/js/jquery-3.7.1.min.js",
               "src/assets/js/moment.min.js",
              "src/assets/js/bootstrap-datetimepicker.min.js",
              "src/assets/plugins/daterangepicker/daterangepicker.js",
              "src/assets/plugins/quill/quill.min.js",
              "src/assets/plugins/select2/js/select2.min.js",
       
              "src/assets/js/bootstrap.bundle.min.js",
              "src/assets/js/feather.min.js",
              "src/assets/js/jquery.slimscroll.min.js",
              "node_modules/apexcharts/dist/apexcharts.min.js",
              "src/assets/plugins/@simonwep/pickr/pickr.es5.min.js",
              "src/assets/js/theme-script.js",
              "src/assets/js/theme-colorpicker.js",
              "src/assets/js/script.js",
              "src/assets/js/jquery.dataTables.min.js"
            ]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2MB",
                  "maximumError": "4MB"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "4kB",
                  "maximumError": "8kB"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular/build:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "AIWareFlow:build:production"
            },
            "development": {
              "buildTarget": "AIWareFlow:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular/build:extract-i18n"
        },
        "test": {
          "builder": "@angular/build:karma",
          "options": {
            "tsConfig": "tsconfig.spec.json",
            "assets": [
              "src/assets",
              {
                "glob": "**/*",
                "input": "public"
              }
            ],
            "styles": [
              "src/assets/css/bootstrap.min.css",
              "src/assets/css/bootstrap-datetimepicker.min.css",
              "src/assets/css/animate.css",
              "src/assets/plugins/select2/css/select2.min.css",
              "src/assets/plugins/tabler-icons/tabler-icons.min.css",
              "src/assets/plugins/fontawesome/css/fontawesome.min.css",
              "src/assets/plugins/fontawesome/css/all.min.css",
              "src/assets/plugins/@simonwep/pickr/themes/nano.min.css",
              "src/assets/css/style.css",
              "node_modules/bootstrap-icons/font/bootstrap-icons.css",
              "src/assets/css/dataTables.bootstrap5.min.css"

            ],
            "scripts": [
              "src/assets/js/jquery-3.7.1.min.js",
              "src/assets/js/moment.min.js",
              "src/assets/js/bootstrap-datetimepicker.min.js",
              "src/assets/plugins/daterangepicker/daterangepicker.js",
              "src/assets/plugins/quill/quill.min.js",
                "src/assets/plugins/select2/js/select2.min.js",
              
              "src/assets/js/bootstrap.bundle.min.js",
              "src/assets/js/feather.min.js",
              "src/assets/js/jquery.slimscroll.min.js",
              "node_modules/apexcharts/dist/apexcharts.min.js",
              "src/assets/plugins/@simonwep/pickr/pickr.es5.min.js",
              "src/assets/js/theme-script.js",
              "src/assets/js/theme-colorpicker.js",
              "src/assets/js/script.js",
              "src/assets/js/jquery.dataTables.min.js"
            ]
          }
        }
      }
    }
  },
  "cli": {
    "analytics": false
  }
}